{
  "type": "mongodb",
  "host": "localhost",
  "database": "research-rpa",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/**/*.entity{.ts,.js}"
  ],
  "subscribers": [
    "src/subscriber/*.subscriber.ts"
  ],
  "migrations": [
    "src/migration/*.migration.ts"
  ],
  "cli": {
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}